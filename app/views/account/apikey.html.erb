<%= flash[:error] %>
<div id="content">
  <h2>Account</h2>
  <%= render "navi" %>
  <div class="account_form">
    <h4>Set API Auth Key</h4>
    <% if @member.auth_key %>
      <p>
        Your API Auth Key is:
        <%= @member.auth_key %>
      </p>
    <% end %>
    <p>
      <%= form_for :account do |f| %>
        <%= f.submit 'Set or Change Auth Key' %>
      <% end %>
    </p>
  </div>
</div>
